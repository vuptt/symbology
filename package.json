{
  "name": "symbology",
  "version": "1.2.7",
  "description": "Generate 50+ different 1D or 2D barcodes in png, eps, or svg format.",
  "keywords": [
    "barcode",
    "datamatrix",
    "code128",
    "qr",
    "postal",
    "1d barcode",
    "2d barcode"
  ],
  "main": "./src/index.js",
  "author": "Josh Shor <https://github.com/jshor>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jshor/symbology.git"
  },
  "binary": {
    "module_name": "symbology",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://symbologyjs.s3-us-west-2.amazonaws.com"
  },
  "dependencies": {
    "nan": "2.x",
    "node-gyp": "3.2.1",
    "node-pre-gyp": "^0.13.0",
    "pngjs": "3.3.0",
    "pngjs-image": "^0.11.6"
  },
  "devDependencies": {
    "aws-sdk": "2.108.0",
    "chai": "4.1.2",
    "coveralls": "2.11.6",
    "istanbul": "0.4.2",
    "mocha": "2.5.3",
    "mocha-lcov-reporter": "1.0.0",
    "sinon": "3.2.1",
    "sinon-chai": "2.13.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "scripts": {
    "dev": "node ./src/test.js",
    "start": "node ./index.js",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./src/**/*.test.js",
    "build": "node-pre-gyp build --target_arch=x64",
    "package-binary": "node-pre-gyp build package --target_arch=x64",
    "publish-binary": "node-pre-gyp publish --target_arch=x64"
  }
}
